<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context=".activities.MyOrderDashboardActivity">


<RelativeLayout
    android:id="@+id/include"
    android:layout_width="match_parent"
    android:layout_height="?attr/actionBarSize"
    android:layout_gravity="center"
    android:background="@color/actionBarBackgroundColor"
    android:gravity="start"
    android:orientation="vertical"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent">


    <ImageView
        android:id="@+id/iv_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true"
        android:paddingStart="@dimen/dp_15"
        android:paddingBottom="@dimen/dp_10"
        android:paddingEnd="@dimen/dp_10"
        android:paddingTop="@dimen/dp_10"
        android:src="@drawable/back" />


    <com.pagin.azul.view.RobotoRegularTextView
        android:id="@+id/name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:gravity="center"
        android:text="@string/my_order_dashboard"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textColor="@color/colorWhite"
        android:textSize="@dimen/text_size_large" />






</RelativeLayout>



<com.pagin.azul.view.RobotoRegularTextView
    android:id="@+id/tvNormalUser"
    android:layout_width="0dp"
    android:layout_height="65dp"
    android:layout_marginStart="32dp"
    android:layout_marginTop="32dp"
    android:layout_marginEnd="32dp"
    android:background="@drawable/bg_sign_in"
    android:gravity="center"
    android:text="@string/dashboard_normal_user"
    android:textAllCaps="false"
    android:textColor="@color/colorWhite"
    android:textSize="@dimen/text_size_medium"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/include" />

<com.pagin.azul.view.RobotoRegularTextView
    android:id="@+id/tvDeliveryWorker"
    android:layout_width="0dp"
    android:layout_height="65dp"
    android:layout_marginStart="32dp"
    android:layout_marginTop="24dp"
    android:layout_marginEnd="32dp"
    android:background="@drawable/bg_sign_in"
    android:gravity="center"
    android:text="@string/dashboard_delivery_worker"
    android:textAllCaps="false"
    android:textColor="@color/colorWhite"
    android:textSize="@dimen/text_size_medium"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.0"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/tvNormalUser" />

<com.pagin.azul.view.RobotoRegularTextView
    android:id="@+id/tvProfessioalWoker"
    android:layout_width="0dp"
    android:layout_height="65dp"
    android:layout_marginStart="32dp"
    android:layout_marginTop="24dp"
    android:layout_marginEnd="32dp"
    android:background="@drawable/bg_sign_in"
    android:gravity="center"
    android:text="@string/dashboard_professional_worker"
    android:textAllCaps="false"
    android:textColor="@color/colorWhite"
    android:textSize="@dimen/text_size_medium"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.0"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/tvDeliveryWorker" />



</androidx.constraintlayout.widget.ConstraintLayout>