<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/dp_5"
        android:background="@drawable/bg_offerdetail_corner"
        android:orientation="vertical">

        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_5">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_5"
                android:layout_marginEnd="@dimen/dp_5"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_60"
                    android:background="@color/colorLightGreen">

                    <com.pagin.azul.view.RobotoRegularTextView
                        android:id="@+id/tv_order"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginStart="@dimen/dp_10"
                        android:text="@string/order_id"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_size_regular" />

                    <com.pagin.azul.view.RobotoSemiBoldTextView
                        android:id="@+id/order_id"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginStart="@dimen/dp_3"
                        android:layout_toRightOf="@+id/tv_order"
                        android:text="1455558"

                        android:textColor="@color/black"
                        android:textSize="@dimen/text_size_medium" />

                    <com.pagin.azul.view.RobotoRegularTextView
                        android:id="@+id/tv_date"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginEnd="@dimen/dp_10"
                        android:layout_toLeftOf="@+id/tv_time"
                        android:text="23,jan,2019"
                        android:textSize="@dimen/text_size_small" />


                    <com.pagin.azul.view.RobotoRegularTextView
                        android:id="@+id/tv_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_centerVertical="true"
                        android:layout_marginEnd="@dimen/dp_10"
                        android:text="04:20 PM"
                        android:textSize="@dimen/text_size_small" />

                </RelativeLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_15"
                    android:orientation="horizontal">

                    <RelativeLayout
                        android:layout_width="@dimen/dp_120"
                        android:layout_height="@dimen/dp_120">

                        <de.hdodenhof.circleimageview.CircleImageView
                            android:id="@+id/user_pic"
                            android:layout_width="@dimen/dp_100"
                            android:layout_height="@dimen/dp_100"
                            android:layout_marginStart="@dimen/dp_10"
                            android:src="@drawable/default_p" />

                        <ImageView
                            android:layout_width="@dimen/dp_30"
                            android:layout_height="@dimen/dp_30"
                            android:layout_alignParentRight="true"
                            android:scaleType="centerCrop"
                            android:src="@drawable/check" />

                    </RelativeLayout>


                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp_15"
                        android:layout_marginTop="@dimen/dp_20">

                        <com.pagin.azul.view.RobotoRegularTextView
                            android:id="@+id/tv_profile_name"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/profilr_name"
                            android:textColor="@color/colortext"
                            android:textSize="@dimen/sp_18" />


                        <RelativeLayout
                            android:id="@+id/rl_maiRaing"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/tv_profile_name">

                            <RelativeLayout
                                android:id="@+id/rl_rat"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/dp_10"
                                android:background="@color/yellow">

                                <com.pagin.azul.view.RobotoRegularTextView
                                    android:id="@+id/tv_rat_num"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_centerVertical="true"
                                    android:layout_marginStart="@dimen/dp_5"
                                    android:text="4.5"
                                    android:textColor="@color/colorWhite"
                                    android:textSize="@dimen/dp_13" />

                                <ImageView
                                    android:layout_width="@dimen/dp_13"
                                    android:layout_height="@dimen/dp_13"
                                    android:layout_centerVertical="true"
                                    android:layout_marginStart="@dimen/dp_3"
                                    android:layout_toRightOf="@+id/tv_rat_num"
                                    android:src="@drawable/star" />
                            </RelativeLayout>

                            <com.pagin.azul.view.RobotoRegularTextView
                                android:id="@+id/tv_view_comments"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/dp_10"
                                android:layout_marginTop="@dimen/dp_10"
                                android:layout_toRightOf="@+id/rl_rat"
                                android:text="@string/rating_view_all"
                                android:textColor="@color/blue_tint"
                                android:textSize="@dimen/dp_13" />
                        </RelativeLayout>

                    </RelativeLayout>
                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_1"
                    android:layout_marginStart="@dimen/dp_15"
                    android:layout_marginTop="@dimen/dp_1"
                    android:layout_marginEnd="@dimen/dp_15"
                    android:background="@color/grey" />


                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_10">

                    <ImageView
                        android:id="@+id/tv_point"
                        android:layout_width="@dimen/dp_30"
                        android:layout_height="@dimen/dp_30"
                        android:layout_centerVertical="true"
                        android:layout_marginStart="32dp"
                        android:src="@drawable/point_1"
                        app:layout_constraintEnd_toStartOf="@+id/view2"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />


                    <ImageView
                        android:id="@+id/tv_point3"
                        android:layout_width="@dimen/dp_30"
                        android:layout_height="@dimen/dp_30"
                        android:layout_centerInParent="true"
                        android:layout_centerVertical="true"
                        android:src="@drawable/point_2"
                        app:layout_constraintEnd_toStartOf="@+id/view4"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toEndOf="@+id/view2"
                        app:layout_constraintTop_toTopOf="parent" />

                    <ImageView
                        android:id="@+id/tv_point4"
                        android:layout_width="@dimen/dp_30"
                        android:layout_height="@dimen/dp_30"
                        android:layout_centerInParent="true"
                        android:layout_centerVertical="true"
                        android:layout_marginEnd="32dp"
                        android:src="@drawable/point_2"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toEndOf="@+id/view3"
                        app:layout_constraintTop_toTopOf="parent" />

                    <View
                        android:id="@+id/view3"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/dp_3"
                        android:layout_centerVertical="true"
                        android:layout_toRightOf="@+id/tv_point"
                        android:background="@color/grey"
                        app:layout_constraintBottom_toTopOf="@+id/tv_dropoffstatus"
                        app:layout_constraintEnd_toStartOf="@+id/tv_point4"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toEndOf="@+id/tv_point1"
                        app:layout_constraintTop_toTopOf="@+id/tv_point1" />

                    <View
                        android:id="@+id/view4"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/dp_3"
                        android:layout_centerVertical="true"
                        android:layout_toRightOf="@+id/tv_point"
                        android:background="@color/grey"
                        app:layout_constraintBottom_toTopOf="@+id/tv_pickupsatatus"
                        app:layout_constraintEnd_toStartOf="@+id/tv_point1"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toEndOf="@+id/tv_point3"
                        app:layout_constraintTop_toTopOf="@+id/tv_point3" />

                    <View
                        android:id="@+id/view2"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/dp_3"
                        android:layout_centerVertical="true"
                        android:layout_toRightOf="@+id/tv_point"
                        android:background="@color/grey"
                        app:layout_constraintBottom_toTopOf="@+id/robotoRegularTextView5"
                        app:layout_constraintEnd_toStartOf="@+id/tv_point3"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toEndOf="@+id/tv_point"
                        app:layout_constraintTop_toTopOf="@+id/tv_point" />

                    <ImageView
                        android:id="@+id/tv_point1"
                        android:layout_width="@dimen/dp_30"
                        android:layout_height="@dimen/dp_30"
                        android:layout_alignParentEnd="true"
                        android:layout_centerVertical="true"
                        android:src="@drawable/point_2"
                        app:layout_constraintEnd_toStartOf="@+id/view3"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toEndOf="@+id/view4"
                        app:layout_constraintTop_toTopOf="parent" />

                    <com.pagin.azul.view.RobotoRegularTextView
                        android:id="@+id/workertopickup"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:text="4.17 KM"
                        android:textColor="@color/colorPink"
                        android:textSize="@dimen/sp_12"
                        app:layout_constraintBottom_toTopOf="@+id/view3"
                        app:layout_constraintEnd_toStartOf="@+id/tv_point4"
                        app:layout_constraintStart_toEndOf="@+id/tv_point1" />

                    <com.pagin.azul.view.RobotoRegularTextView
                        android:id="@+id/pickuptodrop"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginBottom="1dp"
                        android:text="4.17 KM"
                        android:textColor="@color/colorPink"
                        android:textSize="@dimen/sp_12"
                        app:layout_constraintBottom_toTopOf="@+id/view4"
                        app:layout_constraintEnd_toStartOf="@+id/tv_point1"
                        app:layout_constraintStart_toStartOf="@+id/view4" />

                    <com.pagin.azul.view.RobotoRegularTextView
                        android:id="@+id/starttopickup"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:text="4.17 KM"
                        android:textColor="@color/colorPink"
                        android:textSize="@dimen/sp_12"
                        app:layout_constraintBottom_toTopOf="@+id/view2"
                        app:layout_constraintEnd_toEndOf="@+id/view2"
                        app:layout_constraintStart_toStartOf="@+id/view2" />

                    <com.pagin.azul.view.RobotoRegularTextView
                        android:id="@+id/robotoRegularTextView5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="4dp"
                        android:layout_marginEnd="8dp"
                        android:gravity="center"
                        android:text="Start"
                        android:textColor="@color/colortext"
                        android:textSize="@dimen/text_size_small"
                        app:layout_constraintEnd_toStartOf="@+id/view2"
                        app:layout_constraintStart_toStartOf="@+id/tv_point"
                        app:layout_constraintTop_toBottomOf="@+id/tv_point" />




                    <com.pagin.azul.view.RobotoRegularTextView
                        android:id="@+id/tv_pickupsatatus"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_centerInParent="true"
                        android:layout_marginTop="4dp"
                        android:gravity="center"
                        android:text="PickUp"
                        android:textColor="@color/colortext"
                        android:textSize="@dimen/text_size_small"
                        app:layout_constraintEnd_toStartOf="@+id/view4"
                        app:layout_constraintStart_toEndOf="@+id/view2"
                        app:layout_constraintTop_toBottomOf="@+id/tv_point3" />

                    <com.pagin.azul.view.RobotoRegularTextView
                        android:id="@+id/tv_dropoffstatus"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_marginTop="4dp"
                        android:gravity="center"
                        android:text="DropOff"
                        android:textColor="@color/colortext"
                        android:textSize="@dimen/text_size_small"
                        app:layout_constraintEnd_toEndOf="@+id/tv_point1"
                        app:layout_constraintStart_toStartOf="@+id/tv_point1"
                        app:layout_constraintTop_toBottomOf="@+id/tv_point1" />

                    <com.pagin.azul.view.RobotoRegularTextView
                        android:id="@+id/tv_deliveredstatus"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_marginTop="4dp"
                        android:gravity="center"
                        android:text="Delivered"
                        android:textColor="@color/colortext"
                        android:textSize="@dimen/text_size_small"
                        app:layout_constraintEnd_toEndOf="@+id/tv_point4"
                        app:layout_constraintStart_toEndOf="@+id/view3"
                        app:layout_constraintTop_toBottomOf="@+id/tv_point4" />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_1"
                    android:layout_marginStart="@dimen/dp_15"
                    android:layout_marginTop="@dimen/dp_25"
                    android:layout_marginEnd="@dimen/dp_15"
                    android:background="@color/dividerColor" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_15"
                    android:layout_marginTop="@dimen/dp_10"
                    android:layout_marginEnd="@dimen/dp_15">

                    <com.pagin.azul.view.RobotoRegularTextView
                        android:id="@+id/tvPickup"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Pickup Location- "
                        android:textColor="@color/purpalMedium"
                        android:textSize="@dimen/text_size_regular" />

                    <com.pagin.azul.view.RobotoRegularTextView
                        android:id="@+id/pickLoc"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@+id/tvPickup"
                        android:text=" Delhi Golf Club Ltd.,, Dr. Zakir Hussain Road,, New Delhi, Delhi 110003"
                        android:textColor="@color/blacklight"
                        android:textSize="@dimen/text_size_regular" />

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_15"
                    android:layout_marginTop="@dimen/dp_8"
                    android:layout_marginEnd="@dimen/dp_15">

                    <com.pagin.azul.view.RobotoRegularTextView
                        android:id="@+id/tvDrop"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Dropoff Location- "
                        android:textColor="@color/purpalMedium"
                        android:textSize="@dimen/text_size_regular" />

                    <com.pagin.azul.view.RobotoRegularTextView
                        android:id="@+id/dropLoc"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_toEndOf="@+id/tvDrop"
                        android:text="Delhi Golf Club Ltd.,, Dr. Zakir Hussain Road,, New Delhi, Delhi 110003"
                        android:textColor="@color/blacklight"
                        android:textSize="@dimen/text_size_regular" />

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_15"
                    android:layout_marginTop="@dimen/dp_8"
                    android:layout_marginEnd="@dimen/dp_15">

                    <com.pagin.azul.view.RobotoRegularTextView
                        android:id="@+id/tvmsgTxt"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Message- "
                        android:textColor="@color/purpalMedium"
                        android:textSize="@dimen/text_size_regular" />

                    <com.pagin.azul.view.RobotoRegularTextView
                        android:id="@+id/tvMsg"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@+id/tvmsgTxt"
                        android:text="It is a long established fact that a reader will be distracted by the readable"
                        android:textColor="@color/blacklight"
                        android:textSize="@dimen/text_size_regular" />

                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_1"
                    android:layout_marginStart="@dimen/dp_15"
                    android:layout_marginTop="@dimen/dp_15"
                    android:layout_marginEnd="@dimen/dp_15"
                    android:background="@color/colorPink" />


                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_15">

                    <com.pagin.azul.view.RobotoRegularTextView
                        android:id="@+id/tvphn"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentStart="true"
                        android:layout_centerVertical="true"
                        android:layout_marginTop="@dimen/dp_10"
                        android:layout_marginBottom="@dimen/dp_10"
                        android:layout_toLeftOf="@+id/mobile_no"
                        android:text="Pickup Address Phone Number"
                        android:textColor="@color/colortext"
                        android:textSize="@dimen/text_size_regular" />

                    <com.pagin.azul.view.RobotoRegularTextView
                        android:id="@+id/mobile_no"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_centerVertical="true"
                        android:layout_marginTop="@dimen/dp_10"
                        android:layout_marginEnd="@dimen/dp_15"
                        android:text="+000966 253265"
                        android:textColor="@color/blacklight"
                        android:textSize="@dimen/text_size_regular" />

                </RelativeLayout>


                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_1"
                    android:layout_marginStart="@dimen/dp_15"
                    android:layout_marginEnd="@dimen/dp_15"
                    android:background="@color/colorPink" />



                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_15">

                    <com.pagin.azul.view.RobotoRegularTextView
                        android:id="@+id/tv_invoice_details"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginTop="@dimen/dp_10"
                        android:layout_marginBottom="@dimen/dp_10"
                        android:text="@string/invoice_details"
                        android:textColor="@color/purpalMedium"
                        android:textSize="@dimen/text_size_regular" />

                    <com.pagin.azul.view.RobotoRegularTextView
                        android:id="@+id/invoice_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginTop="@dimen/dp_10"
                        android:layout_marginEnd="@dimen/dp_15"
                        android:layout_toStartOf="@+id/invoice_date"
                        android:text="10:00AM,"
                        android:textColor="@color/textgrey"
                        android:textSize="@dimen/text_size_regular" />

                    <com.pagin.azul.view.RobotoRegularTextView
                        android:id="@+id/invoice_date"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_centerVertical="true"
                        android:layout_marginTop="@dimen/dp_10"
                        android:layout_marginEnd="@dimen/dp_15"
                        android:text="12/01/2019"
                        android:textColor="@color/textgrey"
                        android:textSize="@dimen/text_size_regular" />

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_15"
                    android:layout_marginEnd="@dimen/dp_15">

                    <com.pagin.azul.view.RobotoRegularTextView
                        android:id="@+id/tv_delivery_offer"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/delivery_offer"
                        android:textColor="@color/colortext"
                        android:textSize="@dimen/text_size_regular" />

                    <com.pagin.azul.view.RobotoRegularTextView
                        android:id="@+id/delivery_offer"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_centerVertical="true"
                        android:text="100.00SAR"
                        android:textColor="@color/purpalMedium"
                        android:textSize="@dimen/text_size_regular"
                        android:textStyle="bold" />

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_15"
                    android:layout_marginTop="@dimen/dp_8"
                    android:layout_marginEnd="@dimen/dp_15">

                    <com.pagin.azul.view.RobotoRegularTextView
                        android:id="@+id/tv_tax"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginBottom="@dimen/dp_10"
                        android:text="@string/tax"
                        android:textColor="@color/colortext"
                        android:textSize="@dimen/text_size_regular" />

                    <com.pagin.azul.view.RobotoRegularTextView
                        android:id="@+id/tax"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_centerVertical="true"
                        android:text="6.00SAR"
                        android:textColor="@color/purpalMedium"
                        android:textSize="@dimen/text_size_regular"
                        android:textStyle="bold" />

                </RelativeLayout>


                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_15"
                    android:layout_marginTop="@dimen/dp_8"
                    android:layout_marginEnd="@dimen/dp_15"
                    android:layout_marginBottom="@dimen/dp_10">

                    <com.pagin.azul.view.RobotoRegularTextView
                        android:id="@+id/tv_total"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/total"
                        android:textColor="@color/colortext"
                        android:textSize="@dimen/text_size_regular" />

                    <com.pagin.azul.view.RobotoRegularTextView
                        android:id="@+id/total"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_centerVertical="true"
                        android:text="31.00SAR"
                        android:textColor="@color/purpalMedium"
                        android:textSize="@dimen/text_size_regular"
                        android:textStyle="bold" />

                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_1"
                    android:layout_marginStart="@dimen/dp_15"
                    android:layout_marginTop="@dimen/dp_25"
                    android:layout_marginEnd="@dimen/dp_15"
                    android:background="@color/colorLightGreen" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_10"
                    android:layout_marginBottom="@dimen/dp_20"
                    android:orientation="vertical">


                    <com.pagin.azul.view.RobotoRegularTextView
                        android:id="@+id/tvWorkDoneActive"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dp_50"
                        android:layout_marginStart="@dimen/dp_30"
                        android:layout_marginEnd="@dimen/dp_30"
                        android:layout_weight="1"
                        android:background="@drawable/bg_sign_in"
                        android:gravity="center"
                        android:padding="@dimen/dp_5"
                        android:text="@string/msg_track_order"
                        android:textAllCaps="false"
                        android:textColor="@color/white"
                        android:textSize="@dimen/text_size_medium" />


                </LinearLayout>


            </LinearLayout>
        </androidx.core.widget.NestedScrollView>
    </LinearLayout>

</RelativeLayout>
